apiVersion: apps/v1
kind: Deployment
metadata:
  name: plotapp-deployment
  labels:
    app: plotapp
spec:
  replicas: 3
  selector:
    matchLabels:
      app: plotapp
  template:
    metadata:
      labels:
        app: plotapp
    spec:
      containers:
      - name: plotapp
        image:  poojagoudai/plotapp
        ports:
        - containerPort: 75
